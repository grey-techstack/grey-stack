[fortigates]
fortigate01 ansible_host=192.168.1.1 fortios_access_token=your_access_token
fortigate02 ansible_host=192.168.1.2 fortios_access_token=your_access_token

### FortiGate OS Modules ###
[fortigates:vars]
ansible_network_os=fortinet.fortios.fortios

### Specify API connection with HTTP (default SSH)###
[all:vars]
ansible_connection=httpapi
ansible_httpapi_validate_certs=no
ansible_httpapi_use_ssl=yes